<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.management.app.popups.ErrorPopup"
            prefHeight="200.0" prefWidth="800.0">
    <VBox spacing="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20">
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <ImageView fx:id="image" fitHeight="35.0" fitWidth="35.0">
                <Image url="@../images/warning.png"/>
            </ImageView>
            <Label fx:id="title" styleClass="label-big, text-fill-warn" text="%Error"/>
        </HBox>
        <Label fx:id="errorMessageText"/>
    </VBox>
</AnchorPane>
